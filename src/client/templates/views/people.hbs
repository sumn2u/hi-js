{{> header}}
  <img src="assests/images/logo.png" class="logo" alt="hai.js logo">
  <nav class="Panel__Navigation">
    <a href="/index.html">Home</a>
    <a class="selected" >People</a>
    <a href="/companies.html">Organizations</a>
  </nav>
   <section class="Panel__Call-to-action">

    </section>
  <section class="Panel__Timeline Info">
    <ol class="Session__Speaker">
      {{#each people}}
      <li class="Speaker">
        <img class="Speaker__ProfilePicture" src="assests/images/people/{{file.filename}}" alt="profile picture">
        <h2 class="Speaker__Name">{{info.name}}</h2>
        <p class="Speaker__DesignationAndWorkplace">{{info.designation}} at {{info.organization}}</p>
      </li>
      {{~/each}}
    </ol>
  </section>
{{> footer}}