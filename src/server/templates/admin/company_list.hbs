
    <img src="/assests/images/logo.png" class="logo" alt="hai.js logo">
    <nav class="Panel__Navigation">
      <a href="/meetups">Episodes</a>
      <a href="/people">People</a>
      <a class="selected" href="/companies">Organizations</a>
    </nav>

    <section class="Panel__Call-to-action">
      <a href="/companies/form" class="Button Button--Success">Add new company</a>
    </section>

    <section class="Panel__Timeline">
      <ol class="Session__Speaker">
        {{#each companies}}
        <li class="Speaker">
          <img class="Speaker__ProfilePicture" src="/assests/images/companies/{{file.filename}}" alt="profile picture">
          <h2 class="Speaker__Name">{{info.name}}</h2>
          <p class="Speaker__DesignationAndWorkplace">{{info.website}}</p>
          <div class="Speaker__Actions">
            <a href='/companies/delete/{{_id}}' class="Button Button--Danger">Remove</a>
          </div>
        </li>
        {{~/each}}
      </ol>
    </section>
 
